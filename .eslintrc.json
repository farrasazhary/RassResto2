{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals":0,
        "linebreak-style":0,
        "indent": ["error", 2],
        "no-shadow": 0,
        "no-new": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0
    }
}
